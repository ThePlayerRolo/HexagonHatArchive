-- @CloneTrooper1019, 2015
-- Fall Damage Script
--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

hurtSoundId = &quot;rbxassetid://0&quot; -- Sound played if player takes fall damage.
fallingsound = script.Parent:WaitForChild(&quot;falling&quot;)
minTime = 0.5 -- The minimum time a player has to fall in order to begin taking fall damage.
minscreamtime = 1.5
damagePerSec = 20

--------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------

player = game.Players.LocalPlayer--:FindFirstChild(script.Parent.Parent.Parent.Name) -- Finds player
while player == nil do wait(0.5) end
print(&quot;ran&quot;)
character = player.Character or player.CharacterAdded:wait()
humanoid = character:WaitForChild(&quot;Humanoid&quot;)
torso = character:WaitForChild(&quot;Torso&quot;)
falling = false

hurtSound = Instance.new(&quot;Sound&quot;,torso)
hurtSound.SoundId = hurtSoundId

function onStateChanged(_,state)
&#9;if state == Enum.HumanoidStateType.Jumping and not falling then
&#9;&#9;falling = true
&#9;&#9;local highestY = torso.Position.Y
&#9;&#9;local start = tick()
&#9;&#9;local peak = 0
&#9;&#9;while falling do
&#9;&#9;&#9;local y = torso.Position.Y
&#9;&#9;&#9;if y &gt; highestY then
&#9;&#9;&#9;&#9;highestY = y
&#9;&#9;&#9;&#9;peak = tick()
&#9;&#9;&#9;end
&#9;&#9;&#9;wait()
&#9;&#9;&#9;
&#9;&#9;&#9;if tick() - start &gt;= minscreamtime and not fallingsound.IsPlaying then
&#9;&#9;&#9;&#9;fallingsound:Play()
&#9;&#9;&#9;end
&#9;&#9;end
&#9;&#9;fallingsound:Stop()
&#9;&#9;local damage = math.max(0,tick() - peak - minTime) * damagePerSec
&#9;&#9;if damage &gt; 5 then
&#9;&#9;&#9;if humanoid.Health - damage &gt; 0 then -- Only play the hurt sound if the damage isnt fatal.
&#9;&#9;&#9;&#9;hurtSound.Volume = damage/humanoid.MaxHealth
&#9;&#9;&#9;&#9;hurtSound:Play()
&#9;&#9;&#9;end
&#9;&#9;&#9;--humanoid:TakeDamage(damage)
&#9;&#9;end
&#9;elseif state == Enum.HumanoidStateType.Landed then
&#9;&#9;falling = false
&#9;end
end

humanoid.StateChanged:connect(onStateChanged)

