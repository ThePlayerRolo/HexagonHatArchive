				<string name="Name">MinifigureScript</string>
				<ProtectedString name="Source">--Updated for R15 avatars by StarWars
local Tool = script.Parent
local Figure = Tool.Handle

local vCharacter
local myTorso

local clapSound

local sparkles

Tool.Enabled = true

local connection

function glow(hit)
&#9;if hit and hit.Parent and hit.Parent ~= vCharacter then 
&#9;&#9;if hit.Size.X &lt;= 5 and hit.Size.Y &lt;= 5 and hit.Size.Z &lt;= 5 then
&#9;&#9;&#9;hit.Material = Enum.Material.Ice
&#9;&#9;&#9;hit.BrickColor = BrickColor.new(&quot;Bright yellow&quot;)
&#9;&#9;&#9;hit.TopSurface = 0
&#9;&#9;&#9;hit.BottomSurface = 0
&#9;&#9;end
&#9;end
end

function onActivated()
&#9;if Tool.Enabled == false then 
&#9;&#9;return
&#9;end

&#9;Tool.Enabled = false

&#9;vCharacter = Tool.Parent
&#9;myTorso = vCharacter:FindFirstChild(&quot;Torso&quot;) or vCharacter:FindFirstChild(&quot;UpperTorso&quot;)
&#9;if myTorso == nil then 
&#9;&#9;return
&#9;end

&#9;clapSound = Figure:FindFirstChild(&quot;ClapSound&quot;)
&#9;if clapSound then clapSound:Play() end

&#9;sparkles = Instance.new(&quot;Sparkles&quot;)
&#9;sparkles.SparkleColor = Color3.new(1.0, 215/255, 0)
&#9;sparkles.Parent = Figure
&#9;connection = Figure.Touched:connect(glow)
&#9;wait(5.0)
&#9;if connection then connection:disconnect() end
&#9;if sparkles then sparkles:Remove() end
&#9;Tool.Enabled = true
end

function onUnequipped()
&#9;if clapSound then clapSound:Stop() end
&#9;if sparkles then sparkles:Remove() end
&#9;if connection then connection:disconnect() end
end

Tool.Activated:connect(onActivated)
Tool.Unequipped:connect(onUnequipped)

</ProtectedString>
